{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproject.settings",
      "SECRET_KEY": "@your-secret-key",
      "DEBUG": "False"
    }
  }
}
